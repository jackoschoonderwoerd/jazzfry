{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [{
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/public/**",
                    "/*.(png|jpg|jpeg|svg|webp)"
                ]
            }
        }
    ],
    "dataGroups": [{
        "name": "api",
        "urls": [
            "https://firestore.googleapis.com/**",
            "https://identitytoolkit.googleapis.com/**",
            "https://securetoken.googleapis.com/**"
        ],
        "cacheConfig": {
            "strategy": "freshness",
            "maxSize": 0,
            "maxAge": "0u",
            "timeout": "10s"
        }
    }]
}
